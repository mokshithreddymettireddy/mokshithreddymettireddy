#include<stdio.h>
int main()
{
 int num=121,originalnum,reserved=0,remainder;
     originalnum=num;
while(num!=0)
{
  remainder=num%0;
  reserved=reserved*10+remainder;
  num/=10;
}
if (originalnum==reserved)
printf("%d is palindrome"reserved);
else
printf("%d is not palindrome"'reserved);
